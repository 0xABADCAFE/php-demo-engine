{
    "displays": {
        "default": {
            "sType": "ASCIIOverRGB",
            "iWidth": 160,
            "iHeight": 50,
            "iMaxFPS": 30
        }
    },
    "routines": {
        "lines": {
            "sType": "SimpleLine",
            "iPriority": 1,
            "aParameters": {
                "iSpacing": 1,
                "iRate": 16,
                "sFill": "_"
            }
        },
        "doughnut": {
            "sType": "Toroid",
            "iPriority": 2,
            "aParameters": {
                "fMinLuma": 0.0,
                "fLumaFactor": 0.0,
                "iDrawMode":2
            }
        },
        "pulse": {
            "sType": "RGBPulse",
            "iPriority": 0,
            "aParameters": {
            }
        }
    },
    "events": [
        {
            "at": 0.0,
            "on": "routine/lines",
            "do": "enable"
        },
        {
            "at": 1.0,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "iSpacing": 2
            }
        },
        {
            "at": 2.0,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "iSpacing": 3
            }
        },
        {
            "at": 2.5,
            "on": "routine/doughnut",
            "do": "enable"
        },
        {
            "at": 3.0,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "iRate": 2,
                "sFill": "________-__--"
            }
        },
        {
            "at": 4.0,
            "on": "routine/doughnut",
            "do": "update",
            "aParameters": {
                "fMinLuma": 1.0,
                "fLumaFactor": 0.7
            }
        },
        {
            "at": 4.5,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "sFill": "___-_____   ___#__%81"
            }
        },
        {
            "at": 5.0,
            "on": "routine/pulse",
            "do": "enable"
        },
        {
            "at": 7.0,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "sFill": "~"
            }
        },

        {
            "at": 12.0,
            "on": "routine/doughnut",
            "do": "update",
            "aParameters": {
                "iDrawMode": 16
            }
        },

        {
            "at": 14.75,
            "on": "routine/doughnut",
            "do": "update",
            "aParameters": {
                "iDrawMode": 32,
                "fLumaFactor": 0.8
            }
        },


        {
            "at": 18.5,
            "on": "routine/doughnut",
            "do": "update",
            "aParameters": {
                "iDrawMode": 1,
                "fLumaFactor": 0.8
            }
        },

        {
            "at": 18.5,
            "on": "routine/pulse",
            "do": "disable"
        },

        {
            "at": 18.5,
            "on": "routine/lines",
            "do": "update",
            "aParameters": {
                "sFill": "~~~ #____   __##__%81  ."
            }
        },

        {
            "at": 20.0,
            "on": "routine/lines",
            "do": "disable"
        },


        {
            "at": 22.0,
            "do": "end"
        }
    ]
}
