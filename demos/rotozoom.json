{
    "displays": {
        "default": {
            "sType": "DoubleVerticalRGB",
            "iWidth": 160,
            "iHeight": 100,
            "iMaxFPS": 30
        }
    },
    "routines": {
        "zoom": {
            "sType": "2D/Rotozoom",
            "iPriority": 1,
            "aParameters": {
                "sPath": "images/slipped_disc_1.ppm"
            }
        }

    },
    "events": [
        {
            "at": 0.0,
            "on": "routine/zoom",
            "do": "enable"
        },
        {
            "at": 20.0,
            "do": "end"
        }
    ]
}
